/* Morning Steps */
"templates.routine.dry.morning.step1.title" = "Krem Temizleyici";
"templates.routine.dry.morning.step1.why" = "Nemlendirici krem formülü, kuru cildin şiddetle ihtiyaç duyduğu doğal yağları soymadan temizler";
"templates.routine.dry.morning.step1.how" = "Krem temizleyiciyi kuru veya nemli cilde 60 saniye masaj yaparak uygulayın. Durulamak için ılık su kullanın. Nazikçe kurulayın - asla ovuşturmayın.";

"templates.routine.dry.morning.step2.title" = "Nemlendirici Tonik";
"templates.routine.dry.morning.step2.why" = "İlk nemlendirme katmanını sağlar ve cildi sonraki ürünlerden daha fazla nemi emmeye hazırlar";
"templates.routine.dry.morning.step2.how" = "Ellerinizle tampon hareketler yaparak nemli cilde uygulayın. Gliserin, hyaluronik asit veya seramid arayın. Çok kuruysa birden fazla katman uygulayın.";

"templates.routine.dry.morning.step3.title" = "Hyaluronik Asit";
"templates.routine.dry.morning.step3.why" = "Ağırlığının 1000 katına kadar su çeker, kuru ve pul pul dökülen cildi dolgunlaştıran yoğun hidrasyon sağlar";
"templates.routine.dry.morning.step3.how" = "Maksimum etkinlik için nemli cilde uygulayın. 2-3 damla kullanın. Çektiği suyu hapsetmek için hemen ardından nemlendirici sürün.";

"templates.routine.dry.morning.step4.title" = "Zengin Serum";
"templates.routine.dry.morning.step4.why" = "Kuru, susuz kalmış cilde derinlemesine nüfuz eden konsantre besleyici bileşenler sunar";
"templates.routine.dry.morning.step4.how" = "Hyaluronik asitten sonra 3-4 damla uygulayın. Seramidler, peptitler veya besleyici yağlar arayın. Emilmesi için nazikçe tampon hareketler yapın.";

"templates.routine.dry.morning.step5.title" = "Yoğun Nemlendirici";
"templates.routine.dry.morning.step5.why" = "Kuru cildin ana nedeni olan transepidermal su kaybını önlemek için koruyucu bir bariyer oluşturur";
"templates.routine.dry.morning.step5.how" = "Cilt hala nemliyken cömertçe uygulayın. Seramid, shea yağı veya skualan içeren kalın kremleri arayın. Cömert davranın!";

"templates.routine.dry.morning.step6.title" = "Güneş Kremi";
"templates.routine.dry.morning.step6.why" = "UV hasarı cilt bariyerini yıkar ve kuruluğu kötüleştirir. Nemlendirici SPF formüllerini seçin";
"templates.routine.dry.morning.step6.how" = "Krem bazlı SPF 30+ kullanın, jel formüllerden kaçının. Cömertçe uygulayın. Birçok nemlendirici güneş kremi ekstra hidrasyon görevi de görebilir.";

/* Evening Steps */
"templates.routine.dry.evening.step1.title" = "Krem Temizleyici";
"templates.routine.dry.evening.step1.why" = "Akşam temizliği, kuru cildinizin gece boyunca ihtiyaç duyduğu temel nemi korurken günün birikmiş kirlerini temizler";
"templates.routine.dry.evening.step1.how" = "Güneş kremi için önce yağ bazlı temizleyici, ardından krem temizleyici kullanın. Ekstra nazik olun - kuru cilt tahrişe daha yatkındır.";

"templates.routine.dry.evening.step2.title" = "Nemlendirici Tonik";
"templates.routine.dry.evening.step2.why" = "Gece toniği hidrasyon sağlar ve zengin bakımların maksimum emilimi için cildi hazırlar";
"templates.routine.dry.evening.step2.how" = "Ekstra hidrasyon için birden fazla katman uygulayın ('7 cilt' yöntemi). Bir sonraki katmanı eklemeden önce her katmanın emilmesini bekleyin.";

"templates.routine.dry.evening.step3.title" = "Hyaluronik Asit";
"templates.routine.dry.evening.step3.why" = "Cilt onarım modundayken gece hidrasyonu, sabah daha dolgun ve ışıltılı bir ciltle uyanmanızı sağlar";
"templates.routine.dry.evening.step3.how" = "Nemli cilde uygulayın. Çok kuru ciltler için, ekstra destek için nemlendiricinizle bir damla karıştırın.";

"templates.routine.dry.evening.step4.title" = "Zengin Serum";
"templates.routine.dry.evening.step4.why" = "Gece, cildin kendini onardığı zamandır. Zengin serumlar bu süreci desteklemek için besin ve nem sağlar";
"templates.routine.dry.evening.step4.how" = "Hyaluronik asitten sonra cömertçe uygulayın. Sabahkinden daha fazla miktarda kullanabilirsiniz. Ekstra kuru bölgelere odaklanın.";

"templates.routine.dry.evening.step5.title" = "Yoğun Nemlendirici";
"templates.routine.dry.evening.step5.why" = "Gece boyunca nem kaybını önleyen kapatıcı nemlendirici, kuru cilt için en kritik zamandır";
"templates.routine.dry.evening.step5.how" = "Yağdan önceki son adım olarak kalın bir tabaka uygulayın. Özellikle kuru ciltler için uyku maskeleri veya gece kremleri arayın.";

"templates.routine.dry.evening.step6.title" = "Yüz Yağı";
"templates.routine.dry.evening.step6.why" = "Her şeyi besleyici yağlarla mühürler. Son adım, gece boyunca maksimum nem tutulmasını sağlar";
"templates.routine.dry.evening.step6.how" = "3-5 damlayı avuçlarınız arasında ısıtın. Yüz ve boyun bölgesine nazikçe bastırarak uygulayın. Jojoba, argan veya kuşburnu yağı kullanın.";

