"templates.routine.teenAcne.evening.step2.title" = "Salisilik Asitli Tonik";
"templates.routine.teenAcne.evening.step2.why" = "Siz uyurken gözenekleri açmak ve yarının sivilcelerini önlemek için gece boyunca çalışır";
"templates.routine.teenAcne.evening.step2.how" = "Temizledikten sonra uygulayın. Nemlendiriciden önce bir dakika kurumasını bekleyin. Bu, sivilce önleme adımınızdır!";

"templates.routine.teenAcne.evening.step3.title" = "Hafif Nemlendirici";
"templates.routine.teenAcne.evening.step3.why" = "Gece nemlendirmesi, sabah cildinizin aşırı yağ üretmesini önler";
"templates.routine.teenAcne.evening.step3.how" = "Daha iyi emilim için hafif nemli cilde uygulayın. Yağlı cildiniz olsa bile bunu atlamayın!";

"templates.routine.teenAcne.evening.step4.title" = "Leke/Sivilce Bakımı (Spot Treatment)";
