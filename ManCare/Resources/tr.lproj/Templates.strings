"templates.routine.advancedKorean.title" = "İleri Kore Rutini";
"templates.routine.advancedKorean.description" = "Nihai cilt dönüşümü için eksiksiz Kore cilt bakımı ritüeli.";
"templates.routine.advancedKorean.duration" = "25-30 dk";

