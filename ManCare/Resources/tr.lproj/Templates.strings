=================================== */

"templates.routine.koreanGlassSkin.title" = "Kore Cam Cilt Rutini";
"templates.routine.koreanGlassSkin.description" = "Bu kapsamlı Kore cilt bakımı rutini ile o imrenilen nemli, cam görünümlü cilde ulaşın.";
"templates.routine.koreanGlassSkin.duration" = "15-20 dk";

/* Tags */
"templates.routine.koreanGlassSkin.tag1" = "Nemlendirme";
"templates.routine.koreanGlassSkin.tag2" = "Işıltı";
"templates.routine.koreanGlassSkin.tag3" = "Çok Adımlı";
"templates.routine.koreanGlassSkin.tag4" = "Popüler";

/* Benefits */
"templates.routine.koreanGlassSkin.benefit1" = "Derin nemlendirme";
"templates.routine.koreanGlassSkin.benefit2" = "Gelişmiş cilt dokusu";
"templates.routine.koreanGlassSkin.benefit3" = "Işıl ışıl ışıltı";
"templates.routine.koreanGlassSkin.benefit4" = "İnce çizgilerde azalma";

/* Morning Steps */
"templates.routine.koreanGlassSkin.morning.step1.title" = "Su Bazlı Temizleyici";
"templates.routine.koreanGlassSkin.morning.step1.why" = "Gece biriken yağı ve kirleri cildin doğal nem bariyerini soymadan temizler, ürünlerin optimal emilimine hazırlar";
"templates.routine.koreanGlassSkin.morning.step1.how" = "Yüzünüzü ılık suyla ıslatın. Az miktarda ürün uygulayın ve dairesel hareketlerle 30-60 saniye nazikçe masaj yapın. İyice durulayın ve temiz bir havluyla hafifçe kurulayın.";

"templates.routine.koreanGlassSkin.morning.step2.title" = "Tonik";
"templates.routine.koreanGlassSkin.morning.step2.why" = "Temizleme sonrası cildin pH seviyesini dengeler ve sonraki ürünlerin derinlemesine nüfuz etmesi için mükemmel bir ortam yaratır";
"templates.routine.koreanGlassSkin.morning.step2.how" = "Toniği avucunuza veya bir pamuk ped üzerine dökün. Göz çevresinden kaçınarak yüzünüze ve boynunuza nazikçe uygulayın veya yayın. 30 saniye emilmesini bekleyin.";

"templates.routine.koreanGlassSkin.morning.step3.title" = "Esans";
"templates.routine.koreanGlassSkin.morning.step3.why" = "İlk katman hafif nemlendirme sağlar ve cildi serum ve bakımların daha iyi emilmesi için hazırlar";
"templates.routine.koreanGlassSkin.morning.step3.how" = "Birkaç damlayı avucunuza dökün. Yüzünüzün ortasından dışa doğru hareketlerle nazikçe cilde uygulayın. Emilime yardımcı olmak için hafifçe bastırın.";

"templates.routine.koreanGlassSkin.morning.step4.title" = "Serum";
"templates.routine.koreanGlassSkin.morning.step4.why" = "Aydınlatma, yaşlanma karşıtı veya nemlendirme gibi belirli sorunları hedefleyen konsantre aktif bileşenler sunar";
"templates.routine.koreanGlassSkin.morning.step4.how" = "Yüzünüze ve boynunuza 2-3 damla uygulayın. Çekiştirmeden nazikçe bastırarak yedirin. Bir sonraki adıma geçmeden 60 saniye bekleyin.";

"templates.routine.koreanGlassSkin.morning.step5.title" = "Nemlendirici";
"templates.routine.koreanGlassSkin.morning.step5.why" = "Önceki tüm nemlendirme katmanlarını cilde hapseder ve transepidermal su kaybını önlemek için koruyucu bir bariyer oluşturur";
"templates.routine.koreanGlassSkin.morning.step5.how" = "Bir miktar ürünü avuçlarınız arasında ısıtın. Yüz ve boyun bölgesine nazikçe yukarı ve dışa doğru hareketlerle uygulayın. Boynunuzu unutmayın!";

"templates.routine.koreanGlassSkin.morning.step6.title" = "Güneş Kremi";
"templates.routine.koreanGlassSkin.morning.step6.why" = "Gözle görülür yaşlanmanın, koyu lekelerin ve doku sorunlarının %90'ına neden olan UV hasarına karşı cildi korur";
"templates.routine.koreanGlassSkin.morning.step6.how" = "Maruz kalan tüm bölgelere iki parmak uzunluğunda güneş kremi uygulayın. Güneşe çıkmadan 15 dakika önce sürün. Dışarıdayken her 2 saatte bir yenileyin.";

/* Evening Steps */
"templates.routine.koreanGlassSkin.evening.step1.title" = "Yağ Bazlı Temizleyici";
"templates.routine.koreanGlassSkin.evening.step1.why" = "Su bazlı temizleyicilerin etkili bir şekilde çıkaramadığı makyajı, güneş kremini ve yağ bazlı kirleri çözer";
"templates.routine.koreanGlassSkin.evening.step1.how" = "Kuru cilde uygulayın ve 1-2 dakika masaj yapın. Süt kıvamına gelene kadar su ekleyerek emülsiyon haline getirin, ardından ılık suyla iyice durulayın.";

"templates.routine.koreanGlassSkin.evening.step2.title" = "Su Bazlı Temizleyici";
"templates.routine.koreanGlassSkin.evening.step2.why" = "İkinci temizleme, kalan su bazlı kirleri temizler ve maksimum bakım emilimi için cildin tamamen temiz olmasını sağlar";
"templates.routine.koreanGlassSkin.evening.step2.how" = "Nemli cilde uygulayın ve dairesel hareketlerle 60 saniye nazikçe masaj yapın. Ilık suyla iyice durulayın ve hafifçe kurulayın.";

"templates.routine.koreanGlassSkin.evening.step3.title" = "Peeling/Cilt Yenileyici";
"templates.routine.koreanGlassSkin.evening.step3.why" = "Donukluğa ve gözenek tıkanıklığına neden olabilen ölü cilt hücrelerini nazikçe temizleyerek daha parlak ve pürüzsüz bir cilt ortaya çıkarır";
"templates.routine.koreanGlassSkin.evening.step3.how" = "Göz çevresinden kaçınarak temiz ve kuru cilde uygulayın. 10-15 dakika bekletin, ardından durulayın. Her gün değil, haftada 2-3 kez kullanın.";

"templates.routine.koreanGlassSkin.evening.step4.title" = "Kâğıt Maske (Sheet Mask)";
"templates.routine.koreanGlassSkin.evening.step4.why" = "Yoğun hidrasyon ve besinleri, maksimum ışıltı için derinlemesine nüfuz eden konsantre bir bakım olarak sağlar";
"templates.routine.koreanGlassSkin.evening.step4.how" = "Maskeyi temiz yüze uygulayın, hava kabarcıklarını düzeltin. 15-20 dakika dinlenin. Çıkarın ve kalan esansı hafifçe cildinize yedirin. Haftada 2-3 kez kullanın.";

"templates.routine.koreanGlassSkin.evening.step5.title" = "Göz Kremi";
"templates.routine.koreanGlassSkin.evening.step5.why" = "Şişlik, koyu halkalar ve ince çizgileri azaltmak için özel içeriklerle hassas göz bölgesini hedefler";
"templates.routine.koreanGlassSkin.evening.step5.how" = "Yüzük parmağınızı kullanarak pirinç tanesi büyüklüğünde bir miktarı göz çevresi kemiği etrafına nazikçe uygulayın. İçten dışa doğru tampon hareketler yapın. Asla çekmeyin veya çekiştirmeyin.";

"templates.routine.koreanGlassSkin.evening.step6.title" = "Nemlendirici";
"templates.routine.koreanGlassSkin.evening.step6.why" = "Katmanlı bakımların tümünü kilitler ve cildiniz onarım modundayken gece boyunca nemlendirme sağlar";
"templates.routine.koreanGlassSkin.evening.step6.how" = "Yukarı ve dışa doğru hareketlerle cömertçe uygulayın. Akşam nemlendiriciniz sabah kullandığınızdan daha zengin yapıda olabilir.";

