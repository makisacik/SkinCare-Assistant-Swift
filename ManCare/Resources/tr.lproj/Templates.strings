MARK: - Acne Clear Routine
=================================== */

"templates.routine.acneClear.title" = "Sivilce Temizleme Rutini";
"templates.routine.acneClear.description" = "Temiz ve sağlıklı bir cilt için bu hedefe yönelik rutin ile sivilcelere karşı savaşın ve gelecekteki oluşumları önleyin.";
"templates.routine.acneClear.duration" = "8-12 dk";

/* Tags */
"templates.routine.acneClear.tag1" = "Sivilce";
"templates.routine.acneClear.tag2" = "Salisilik Asit";
"templates.routine.acneClear.tag3" = "Nazik";
"templates.routine.acneClear.tag4" = "Etkili";

/* Benefits */
"templates.routine.acneClear.benefit1" = "Aktif sivilceleri azaltır";
"templates.routine.acneClear.benefit2" = "Yeni sivilceleri önler";
"templates.routine.acneClear.benefit3" = "Gözenekleri küçültür";
"templates.routine.acneClear.benefit4" = "İltihabı yatıştırır";

/* Morning Steps */
"templates.routine.acneClear.morning.step1.title" = "Nazik Temizleyici";
"templates.routine.acneClear.morning.step1.why" = "Aşırı yağı ve kirleri cildi aşırı kurutmadan temizler, bu da daha fazla yağ üretimine neden olarak sivilceleri kötüleştirebilir";
"templates.routine.acneClear.morning.step1.how" = "Ilık su kullanın ve 60 saniye nazikçe masaj yapın. Sıcak su ve sert ovalamadan kaçının. Temiz bir havluyla hafifçe kurulayın.";

"templates.routine.acneClear.morning.step2.title" = "Salisilik Asitli Tonik";
"templates.routine.acneClear.morning.step2.why" = "Yağ ve kirleri çözmek için gözeneklere nüfuz eder, siyah nokta ve sivilcelere yol açan tıkanıklıkları önler";
"templates.routine.acneClear.morning.step2.how" = "Pamuk ped veya doğrudan avuç içine uygulayın. Yüzünüzde, özellikle sivilce eğilimli bölgelere odaklanarak nazikçe yayın. BHA'ya yeni başlıyorsanız gün aşırı kullanın.";

"templates.routine.acneClear.morning.step3.title" = "Niasinamid Serumu";
"templates.routine.acneClear.morning.step3.why" = "İltihabı azaltır, yağ üretimini düzenler ve sivilce sonrası izlerin tahriş etmeden solmasına yardımcı olur";
"templates.routine.acneClear.morning.step3.how" = "Yüzünüze ve boynunuza 2-3 damla uygulayın. Emilmesi için nazikçe tampon hareketler yapın. Nemlendiriciden önce 60 saniye bekleyin. Sabah ve akşam kullanılabilir.";

"templates.routine.acneClear.morning.step4.title" = "Hafif Nemlendirici";
"templates.routine.acneClear.morning.step4.why" = "Gözenekleri tıkamadan cilt bariyeri sağlığını korur. Yağlı, sivilce eğilimli cilt bile aşırı yağ üretimini önlemek için nemlendirmeye ihtiyaç duyar";
"templates.routine.acneClear.morning.step4.how" = "Jel veya su bazlı bir nemlendirici kullanın. İnce bir tabaka uygulayın ve tamamen emilmesini bekleyin. 'Komedojenik olmayan' etiketlerini arayın.";

"templates.routine.acneClear.morning.step5.title" = "Güneş Kremi";
"templates.routine.acneClear.morning.step5.why" = "İyileşen sivilcelerden koyu lekeler oluşmasını engeller ve cildi hasardan korur. Birçok sivilce bakımı güneşe hassasiyeti artırır";
"templates.routine.acneClear.morning.step5.how" = "Yağsız, komedojenik olmayan SPF 30+ uygulayın. İki parmak uzunluğunda kullanın ve dışarıda zaman geçiriyorsanız yenileyin.";

/* Evening Steps */
"templates.routine.acneClear.evening.step1.title" = "Nazik Temizleyici";
"templates.routine.acneClear.evening.step1.why" = "Günün birikmiş yağı, kiri ve bakterileri cildi soymadan veya tahriş etmeden temizler";
"templates.routine.acneClear.evening.step1.how" = "Ilık suyla 60 saniye masaj yapın. Aktif sivilcelerin etrafına nazik davranın. İyice durulayın ve hafifçe kurulayın.";

"templates.routine.acneClear.evening.step2.title" = "Salisilik Asitli Tonik";
"templates.routine.acneClear.evening.step2.why" = "Akşam uygulaması, BHA'nın gece boyunca çalışmasını, gözenekleri açmasını ve siz uyurken yeni sivilceleri önlemesini sağlar";
"templates.routine.acneClear.evening.step2.how" = "Temiz ve kuru cilde uygulayın. T bölgesi ve sivilce eğilimli bölgelere odaklanın. Bu gece retinol gibi başka güçlü aktifler kullanıyorsanız atlayın.";

"templates.routine.acneClear.evening.step3.title" = "Niasinamid Serumu";
"templates.routine.acneClear.evening.step3.why" = "Günün yorgunluğundan kaynaklanan iltihabı yatıştırır, kızarıklığı azaltır ve sivilceleri kontrol etmek için BHA ile sinerjik çalışır";
"templates.routine.acneClear.evening.step3.how" = "Tonikten sonra 2-3 damla uygulayın. Nazikçe tampon hareketlerle emdirin. Bir sonraki adımdan önce bir dakika bekleyin.";

"templates.routine.acneClear.evening.step4.title" = "Leke/Sivilce Bakımı (Spot Treatment)";
"templates.routine.acneClear.evening.step4.why" = "Daha hızlı iyileşme için konsantre sivilce karşıtı bileşenleri doğrudan aktif sivilcelere ulaştırır";
"templates.routine.acneClear.evening.step4.how" = "Sadece aktif sivilcelere küçük bir miktar uygulayın, tüm yüze değil. Temiz bir parmak veya pamuklu çubuk kullanın. Tamamen kurumasını bekleyin.";

"templates.routine.acneClear.evening.step5.title" = "Hafif Nemlendirici";
"templates.routine.acneClear.evening.step5.why" = "Bakımları kilitler ve cildin aşırı kurumasını önler, bu da ironik olarak sivilceleri kötüleştirebilir";
"templates.routine.acneClear.evening.step5.how" = "Leke bakımı kuruduktan sonra uygulayın. İnce bir tabaka kullanın. Sivilce eğilimli ciltler için jel veya su bazlı formülleri seçin.";

