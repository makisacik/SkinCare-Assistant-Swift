/* Benefits */
"templates.routine.advancedKorean.benefit1" = "Tam cilt dönüşümü";
"templates.routine.advancedKorean.benefit2" = "Maksimum nemlendirme";
"templates.routine.advancedKorean.benefit3" = "Gelişmiş yaşlanma karşıtı bakım";
"templates.routine.advancedKorean.benefit4" = "Lüks deneyim";

/* Morning Steps */
"templates.routine.advancedKorean.morning.step1.title" = "Su Bazlı Temizleyici";
"templates.routine.advancedKorean.morning.step1.why" = "Sabah ikinci temizleme, gece biriken yağları temizler ve cildi birden fazla bakım katmanına hazırlar";
"templates.routine.advancedKorean.morning.step1.how" = "Nazik, düşük pH'lı temizleyici kullanın. 60 saniye masaj yapın. Başarılı katmanlamanın temeli temiz bir ciltle başlar.";

"templates.routine.advancedKorean.morning.step2.title" = "Tonik";
"templates.routine.advancedKorean.morning.step2.why" = "pH'ı dengeler ve takip edecek birçok bakım katmanı için optimal ortamı yaratır";
"templates.routine.advancedKorean.morning.step2.how" = "İstenirse '7 cilt' yöntemi kullanılarak toniği ellerle cilde yedirin. Bu çok adımlı ritüeldeki ilk hidrasyon katmanıdır.";

"templates.routine.advancedKorean.morning.step3.title" = "Esans";
"templates.routine.advancedKorean.morning.step3.why" = "K-güzelliğinin çekirdeği: sonraki ürünlerin emilimini artıran hafif nemlendirme sağlar";
"templates.routine.advancedKorean.morning.step3.how" = "Avuç içlerine dökün ve cilde nazikçe bastırın. Bu sulu katman, o cam cilt ışıltısının anahtarıdır.";

"templates.routine.advancedKorean.morning.step4.title" = "Bakım Esansı (Treatment Essence)";
"templates.routine.advancedKorean.morning.step4.why" = "Cilt yenileme ve aydınlatma için fermente edilmiş bileşenler ve aktif bileşenler içerir";
"templates.routine.advancedKorean.morning.step4.how" = "Yüzünüze 2-3 damla uygulayın. Bakım esansları normal esanslara göre daha konsantredir. Premium K-güzellik sırrıdır.";

"templates.routine.advancedKorean.morning.step5.title" = "Serum";
"templates.routine.advancedKorean.morning.step5.why" = "Belirli sorunlar için hedefe yönelik bakım - aydınlatma, yaşlanma karşıtı veya hidrasyon";
"templates.routine.advancedKorean.morning.step5.how" = "2-3 damla uygulayın. Emilmesi için bastırın ve tampon hareketler yapın. Farklı sorunlar için birden fazla serum kullanabilirsiniz.";

"templates.routine.advancedKorean.morning.step6.title" = "Ampul";
"templates.routine.advancedKorean.morning.step6.why" = "Maksimum sonuçlar için en yüksek aktif bileşen seviyelerine sahip en konsantre bakım ürünüdür";
"templates.routine.advancedKorean.morning.step6.how" = "Sadece 1-2 damla kullanın - çok güçlüdür. Nazikçe yedirin. Ana sorunlar için ayırın. Genellikle özel günler veya bakımlar için kullanılır.";

"templates.routine.advancedKorean.morning.step7.title" = "Göz Kremi";
"templates.routine.advancedKorean.morning.step7.why" = "Hassas göz çevresi, yüz nemlendiricisinden önce özel bir bakıma ihtiyaç duyar";
"templates.routine.advancedKorean.morning.step7.how" = "Yüzük parmağınızı kullanarak yörünge kemiği çevresine küçük bir miktar nazikçe dokunun. Hassas cildi asla çekmeyin veya çekiştirmeyin.";

"templates.routine.advancedKorean.morning.step8.title" = "Nemlendirici";
"templates.routine.advancedKorean.morning.step8.why" = "Tüm bakım katmanlarını mühürler ve gün boyunca kalıcı hidrasyon sağlar";
"templates.routine.advancedKorean.morning.step8.how" = "Önceki tüm adımları kilitlemek için uygulayın. Kore nemlendiricileri genellikle hafif bir bitiş için jel bazlıdır.";

"templates.routine.advancedKorean.morning.step9.title" = "Güneş Kremi";
"templates.routine.advancedKorean.morning.step9.why" = "Son adım, tüm bu cilt bakımı yatırımını korur. K-güzelliği, dünyanın en kozmetik açıdan zarif güneş kremlerine sahiptir";
"templates.routine.advancedKorean.morning.step9.how" = "Son adım olarak cömertçe uygulayın. Kore güneş kremleri, makyaj altında iyi çalışan zarif, yağsız bitişleriyle ünlüdür.";

/* Evening Steps */
"templates.routine.advancedKorean.evening.step1.title" = "Yağ Bazlı Temizleyici";
"templates.routine.advancedKorean.evening.step1.why" = "İlk temizleme, makyajı, güneş kremini ve yağ bazlı kirleri çözer. Çift temizleme yönteminin temelidir";
"templates.routine.advancedKorean.evening.step1.how" = "Kuru cilde 1-2 dakika masaj yapın. Süt kıvamına gelene kadar su ekleyerek emülsiyon haline getirin, ardından iyice durulayın.";

"templates.routine.advancedKorean.evening.step2.title" = "Su Bazlı Temizleyici";
"templates.routine.advancedKorean.evening.step2.why" = "İkinci temizleme, mükemmel temiz bir cilt için kalan su bazlı kirleri temizler";
"templates.routine.advancedKorean.evening.step2.how" = "Islak cilde masaj yapın. Çift temizleme, gerçekten temiz, ürüne hazır bir cilt için K-güzelliğinde olmazsa olmazdır.";

"templates.routine.advancedKorean.evening.step3.title" = "Peeling/Cilt Yenileyici";
"templates.routine.advancedKorean.evening.step3.why" = "Tüm bakım ürünlerinin etkili bir şekilde nüfuz edebilmesi için ölü cilt hücrelerini temizler";
"templates.routine.advancedKorean.evening.step3.how" = "Kimyasal peeling (AHA/BHA) uygulayın. 10-15 dakika bekletin, durulayın. Her gün değil, haftada 2-3 kez kullanın.";

"templates.routine.advancedKorean.evening.step4.title" = "Tonik";
"templates.routine.advancedKorean.evening.step4.why" = "Peeling sonrası pH'ı dengeler ve bakım katmanları için hazırlar";
"templates.routine.advancedKorean.evening.step4.how" = "Cömertçe cilde yedirin. Birden fazla katman yapabilirsiniz. Akşam rutini sabahtan daha fazla katman içerir.";

"templates.routine.advancedKorean.evening.step5.title" = "Esans";
"templates.routine.advancedKorean.evening.step5.why" = "Yeni peeling yapılmış cilde ilk hidrasyon dalgasını ve bakım bileşenlerini ulaştırır";
"templates.routine.advancedKorean.evening.step5.how" = "Cilde nazikçe bastırın. Yeni peeling yapılmış cilt, gelişmiş sonuçlar için esansı daha etkili bir şekilde emer.";

"templates.routine.advancedKorean.evening.step6.title" = "Bakım Esansı (Treatment Essence)";
"templates.routine.advancedKorean.evening.step6.why" = "Cildin onarım döngüsü sırasında bakım esanslarının yenilenme sihrini yapması için gece optimal zamandır";
"templates.routine.advancedKorean.evening.step6.how" = "Yüzünüze 2-3 damla uygulayın. Bunlar siz uyurken çalışarak sabah ışıltısını sağlar.";

"templates.routine.advancedKorean.evening.step7.title" = "Serum";
"templates.routine.advancedKorean.evening.step7.why" = "Akşam serumları daha yoğun olabilir - gece onarımı için retinol, peptitler veya yoğun hidrasyon içerir";
"templates.routine.advancedKorean.evening.step7.how" = "Hedefe yönelik serum uygulayın. Sabahkinden farklı bir serum kullanabilirsiniz. Gece, yaşlanma karşıtı aktifler içindir.";

"templates.routine.advancedKorean.evening.step8.title" = "Ampul";
"templates.routine.advancedKorean.evening.step8.why" = "Konsantre bakım, cilt en yoğun onarım modundayken gece boyunca çalışır";
"templates.routine.advancedKorean.evening.step8.how" = "En güçlü bakımınızdan 1-2 damla kullanın. Bu, özel cilt ihtiyaçları için premium adımınızdır.";

"templates.routine.advancedKorean.evening.step9.title" = "Kâğıt Maske (Sheet Mask)";
"templates.routine.advancedKorean.evening.step9.why" = "Yoğun hidrasyon ve tedaviyi rahatlatıcı bir ritüel içinde sağlayan haftalık bakım. K-güzelliğinin imzası niteliğindedir";
"templates.routine.advancedKorean.evening.step9.how" = "Esans/serumlardan sonra, nemlendiriciden önce uygulayın. 15-20 dakika rahatlayın. Kalan esansı cilde yedirin. Haftada 2-3 kez kullanın.";

"templates.routine.advancedKorean.evening.step10.title" = "Göz Kremi";
"templates.routine.advancedKorean.evening.step10.why" = "Gece göz kremi, gece boyunca yenilenme için yoğun içeriklerle daha zengin olabilir";
"templates.routine.advancedKorean.evening.step10.how" = "Tüm göz çevresi bölgesine nazikçe dokunun. Sabahtan daha cömert miktarda kullanabilirsiniz. İnce çizgiler gibi sorunlara odaklanın.";

"templates.routine.advancedKorean.evening.step11.title" = "Nemlendirici";
"templates.routine.advancedKorean.evening.step11.why" = "Son kapatıcı katman, tüm bakımları mühürler ve gece boyunca hidrasyon sağlar";
"templates.routine.advancedKorean.evening.step11.how" = "Cömertçe uygulayın. Ekstra gece desteği için haftada 2-3 kez uyku maskesi kullanabilirsiniz. Işıl ışıl bir ciltle uyanın.";

