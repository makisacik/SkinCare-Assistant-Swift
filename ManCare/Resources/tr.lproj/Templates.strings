"templates.routine.antiAging.benefit4" = "Kolajeni uyarır";

/* Morning Steps */
"templates.routine.antiAging.morning.step1.title" = "Nazik Temizleyici";
"templates.routine.antiAging.morning.step1.why" = "Daha kuru ve hassas olma eğiliminde olan olgun cildi soymadan gece birikmiş kirleri temizler";
"templates.routine.antiAging.morning.step1.how" = "Ilık suyla krem veya yağ bazlı bir temizleyici kullanın. 60 saniye nazikçe masaj yapın, iyice durulayın ve hafifçe kurulayın.";

"templates.routine.antiAging.morning.step2.title" = "C Vitamini Serumu";
"templates.routine.antiAging.morning.step2.why" = "Güçlü bir antioksidandır; cildi aydınlatır, tonu eşitler, kolajen üretimini destekler ve UV ile kirlilikten kaynaklanan serbest radikal hasarına karşı korur";
"templates.routine.antiAging.morning.step2.how" = "Her sabah temiz cilde 3-4 damla uygulayın. Yüzünüze ve boynunuza nazikçe bastırarak yedirin. Bir sonraki adımdan önce 60-90 saniye bekleyin. Serin ve karanlık bir yerde saklayın.";

"templates.routine.antiAging.morning.step3.title" = "Hyaluronik Asit";
"templates.routine.antiAging.morning.step3.why" = "Ağırlığının 1000 katına kadar su çekip tutar, ince çizgileri dolgunlaştırır ve cildin genç sıçrama özelliğini korur";
"templates.routine.antiAging.morning.step3.how" = "En iyi emilim için hafif nemli cilde uygulayın. 2-3 damla kullanın ve nemi hapsetmek için hemen ardından nemlendirici sürün.";

"templates.routine.antiAging.morning.step4.title" = "Zengin Nemlendirici";
"templates.routine.antiAging.morning.step4.why" = "Yaşla birlikte azalan temel lipitleri ve seramidleri sağlar, cilt bariyerini güçlendirir ve nem kaybını önler";
"templates.routine.antiAging.morning.step4.how" = "Cömert bir miktarı avuçlarınız arasında ısıtın. Yukarı doğru hareketlerle cildinize bastırarak uygulayın. Kuruluk ve çizgi eğilimli bölgelere odaklanın.";

"templates.routine.antiAging.morning.step5.title" = "Güneş Kremi";
"templates.routine.antiAging.morning.step5.why" = "UV maruziyeti gözle görülür yaşlanmanın %90'ına neden olur. Günlük SPF, uygulayabileceğiniz en etkili yaşlanma karşıtı önlemdir";
"templates.routine.antiAging.morning.step5.how" = "Son adım olarak geniş spektrumlu SPF 50 uygulayın. İki parmak uzunluğunda kullanın ve dışarıdaysanız her 2 saatte bir yenileyin. Bulutlu günlerde bile atlamayın.";

/* Evening Steps */
"templates.routine.antiAging.evening.step1.title" = "Nazik Temizleyici";
"templates.routine.antiAging.evening.step1.why" = "Cildinizin koruyucu bariyerini bozmadan günün güneş kremini, kirliliğini ve makyajını temizler";
"templates.routine.antiAging.evening.step1.how" = "Güneş kremi kullandıysanız çift temizleme yapın: önce yağ bazlı temizleyici, ardından su bazlı. Her biri için 60 saniye masaj yapın, ılık suyla durulayın.";

"templates.routine.antiAging.evening.step2.title" = "Retinol Bakımı";
"templates.routine.antiAging.evening.step2.why" = "Hücre yenilenmesini artıran, kolajeni destekleyen, kırışıklıkları azaltan ve cilt tonunu eşitleyen, yaşlanma karşıtı altın standart bileşendir";
"templates.routine.antiAging.evening.step2.how" = "Göz çevresinden kaçınarak kuru cilde bezelye büyüklüğünde uygulayın. Haftada 2 kez başlayın, kademeli olarak artırın. Ertesi gün daima güneş kremi kullanın. Nemlendiriciden önce 20 dakika bekleyin.";

"templates.routine.antiAging.evening.step3.title" = "Peptit Serumu";
"templates.routine.antiAging.evening.step3.why" = "Peptitler, cilde daha fazla kolajen ve elastin üretmesi için sinyal göndererek sıkılığı artırır ve ince çizgilerin görünümünü azaltır";
"templates.routine.antiAging.evening.step3.how" = "Retinol emildikten sonra 2-3 damla uygulayın. Yüzünüze ve boynunuza nazikçe tampon hareketler yapın. Peptitler, gelişmiş sonuçlar için retinol ile iyi çalışır.";

"templates.routine.antiAging.evening.step4.title" = "Göz Kremi";
"templates.routine.antiAging.evening.step4.why" = "Göz çevresi cildi daha incedir ve yaşlanma belirtilerini ilk gösterir. Özel içerikler kaz ayaklarını, koyu halkaları ve ince çizgileri hedefler";
"templates.routine.antiAging.evening.step4.how" = "Yüzük parmağınızı kullanarak pirinç tanesi miktarını göz çevresi kemiği etrafına nazikçe tampon hareketlerle uygulayın. İçten dışa doğru uygulayın. Asla çekiştirmeyin.";

"templates.routine.antiAging.evening.step5.title" = "Zengin Nemlendirici";
"templates.routine.antiAging.evening.step5.why" = "Gece, cildin kendini onardığı zamandır. Zengin bir nemlendirici, besleyici içerikler ve derin hidrasyon ile bu süreci destekler";
"templates.routine.antiAging.evening.step5.how" = "Tüm bakımları hapsetmek için son adım olarak cömertçe uygulayın. Sabah nemlendiricisinden biraz daha zengin olabilir. Boyun ve göğsü unutmayın.";

