"templates.routine.sensitive.title" = "Hassas Cilt Yatıştırıcı";
"templates.routine.sensitive.description" = "Hassas ve reaktif cilt tipleri için tasarlanmış nazik, sakinleştirici bir rutin.";
"templates.routine.sensitive.duration" = "6-8 dk";

/* Tags */
"templates.routine.sensitive.tag1" = "Nazik";
