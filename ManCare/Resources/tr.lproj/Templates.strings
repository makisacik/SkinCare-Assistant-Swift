"templates.routine.oily.benefit4" = "Sivilce oluşumunu önler";

/* Morning Steps */
"templates.routine.oily.morning.step1.title" = "Jel Temizleyici";
"templates.routine.oily.morning.step1.why" = "Daha fazla yağ üretimine yol açabilecek aşırı kurumaya neden olmadan fazla sebumu ve gece biriken yağı temizler";
"templates.routine.oily.morning.step1.how" = "Nazik bir jel veya köpüren temizleyici kullanın. Ilık suyla 60 saniye masaj yapın. Yağ bezlerini uyaran sıcak sudan kaçının.";

"templates.routine.oily.morning.step2.title" = "BHA Tonik";
"templates.routine.oily.morning.step2.why" = "Salisilik asit, yağa doymuş gözeneklere nüfuz ederek tıkanıklıkları ve sivilceleri önlerken parlamayı kontrol eder";
"templates.routine.oily.morning.step2.how" = "Temiz cilde pamuk ped veya ellerinizle uygulayın. T bölgesine odaklanın. Tahrişi önlemek için BHA'ya yeni başlıyorsanız gün aşırı başlayın.";

"templates.routine.oily.morning.step3.title" = "Niasinamid Serumu";
"templates.routine.oily.morning.step3.why" = "Yağ üretimini %50'ye kadar düzenlediği klinik olarak kanıtlanmıştır ve gözenek görünümünü azaltır";
"templates.routine.oily.morning.step3.how" = "Tüm yüze 2-3 damla uygulayın. BHA ile iyi uyum sağlar. 4-8 hafta boyunca tutarlı günlük kullanımla en iyi sonucu verir.";

"templates.routine.oily.morning.step4.title" = "Hafif Nemlendirici";
"templates.routine.oily.morning.step4.why" = "Yağlı cilt bile nemlendirmeye ihtiyaç duyar. Nemlendiriciyi atlamak, cildin telafi etmek için DAHA FAZLA yağ üretmesine neden olur";
"templates.routine.oily.morning.step4.how" = "Jel veya su bazlı formül kullanın. Cilt hafif nemliyken ince bir tabaka uygulayın. Yağsız, komedojenik olmayan etiketlerini arayın.";

"templates.routine.oily.morning.step5.title" = "Yağsız Güneş Kremi";
"templates.routine.oily.morning.step5.why" = "Yağ eklemeden koruma sağlar. Birçok sivilce bakımı güneşe hassasiyeti artırdığı için SPF hayati önem taşır";
"templates.routine.oily.morning.step5.how" = "Matlaştırıcı veya jel yapılı güneş kremlerini tercih edin. Son adım olarak SPF 30+ uygulayın. Dışarıdaysanız yenileyin.";

/* Evening Steps */
"templates.routine.oily.evening.step1.title" = "Jel Temizleyici";
"templates.routine.oily.evening.step1.why" = "Günün yağı, güneş kremi ve kirlerini temizler, gece boyunca gözenekleri tıkamalarını ve sivilceye neden olmalarını önler";
"templates.routine.oily.evening.step1.how" = "Yoğun güneş kremi kullandıysanız çift temizleme yapın: önce yağ bazlı temizleyici, ardından jel temizleyici. 60 saniye boyunca iyice masaj yapın.";

"templates.routine.oily.evening.step2.title" = "BHA Tonik";
"templates.routine.oily.evening.step2.why" = "Akşam uygulaması, salisilik asidin gece boyunca çalışmasını, gözenekleri açmasını ve sabah parlamasını önlemesini sağlar";
"templates.routine.oily.evening.step2.how" = "Temiz ve kuru cilde uygulayın. Cilt tolerans gösterirse akşamları günlük olarak kullanılabilir. Aynı gece retinol kullanıyorsanız atlayın.";

"templates.routine.oily.evening.step3.title" = "Niasinamid Serumu";
"templates.routine.oily.evening.step3.why" = "Cilt onarım halindeyken gece niasinamid, yağ üretimini ve iltihabı azaltır";
"templates.routine.oily.evening.step3.how" = "Tonikten sonra uygulayın. BHA ile güvenle katmanlanabilir. Gündüz yağlanmasını kontrol etmek için geceleri kullanın.";

"templates.routine.oily.evening.step4.title" = "Hafif Nemlendirici";
"templates.routine.oily.evening.step4.why" = "Gece boyunca nem kaybını önler, bu da ertesi gün aşırı yağ üretimine yol açabilir";
"templates.routine.oily.evening.step4.how" = "İnce bir tabaka uygulayın. Akşam nemlendiricisi sabahkiyle aynı olabilir veya gerekiyorsa biraz daha nemlendirici olabilir.";

"templates.routine.oily.evening.step5.title" = "Kil Maskesi";
"templates.routine.oily.evening.step5.why" = "Haftalık derin bakım, fazla yağı emer, gözenekleri açar ve daha temiz bir cilt için derinlemesine temizlik sağlar";
"templates.routine.oily.evening.step5.how" = "Temizledikten sonra haftada 1-2 kez kullanın. İnce bir tabaka uygulayın, 10-15 dakika bekletin. Tamamen kurutmayın. İyice durulayın.";

