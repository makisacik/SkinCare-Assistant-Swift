"templates.routine.sensitive.tag3" = "Parfümsüz";
"templates.routine.sensitive.tag4" = "Hassas";

/* Benefits */
"templates.routine.sensitive.benefit1" = "Tahrişi azaltır";
"templates.routine.sensitive.benefit2" = "Cilt bariyerini güçlendirir";
"templates.routine.sensitive.benefit3" = "Hassas ciltte nazik";
"templates.routine.sensitive.benefit4" = "Sakinleştirici içerikler";

/* Morning Steps */
"templates.routine.sensitive.morning.step1.title" = "Krem Temizleyici";
"templates.routine.sensitive.morning.step1.why" = "Hassas cildi soymadan veya tahriş etmeden temizler. Krem formüller, bariyeri bozabilen köpürenlerden daha naziktir";
"templates.routine.sensitive.morning.step1.how" = "Ilık (sıcak değil) su kullanın. Sert ovuşturmadan nazikçe masaj yapın. İyice durulayın ve yumuşak bir havluyla hafifçe kurulayın (ovalamayın).";

"templates.routine.sensitive.morning.step2.title" = "Yatıştırıcı Tonik";
"templates.routine.sensitive.morning.step2.why" = "Hafif nemlendirme sağlarken iltihabı ve kızarıklığı yatıştırır. Centella, yeşil çay veya kolloidal yulaf ezmesi içerenleri arayın";
"templates.routine.sensitive.morning.step2.how" = "Sürtünmeye ve tahrişe neden olabilecek pamuk ped kullanmaktan kaçının. Temiz ellerinizle nazikçe tampon hareketlerle cilde uygulayın.";

"templates.routine.sensitive.morning.step3.title" = "Hyaluronik Asit";
"templates.routine.sensitive.morning.step3.why" = "Nazik, tahriş etmeyen hidrasyon sağlar. Hassasiyet veya sivilce riski olmadan tüm cilt tipleri için uygundur";
"templates.routine.sensitive.morning.step3.how" = "Maksimum emilim için nemli cilde uygulayın. Nazik tampon hareketler kullanın. Nemi hapsetmek için ardından nemlendirici sürün.";

"templates.routine.sensitive.morning.step4.title" = "Bariyer Onarıcı Krem";
"templates.routine.sensitive.morning.step4.why" = "Seramid ve niasinamid ile bozulmuş cilt bariyerini güçlendirir. Zamanla hassasiyeti azaltır ve tahriş edicilere karşı korur";
"templates.routine.sensitive.morning.step4.how" = "Önceki ürünleri kilitlemek için cömert bir katman uygulayın. Seramid içeren parfümsüz formülleri arayın. Sabah ve akşam kullanın.";

"templates.routine.sensitive.morning.step5.title" = "Mineral Güneş Kremi";
"templates.routine.sensitive.morning.step5.why" = "Çinko oksit veya titanyum dioksit içeren fiziksel güneş kremleri, kimyasal filtrelere göre tahrişe daha az neden olur";
"templates.routine.sensitive.morning.step5.how" = "Son adım olarak SPF 30+ uygulayın. Mineral güneş kremleri cilde nüfuz etmek yerine üzerinde kalır, bu da onları hassas cilt için ideal yapar.";

/* Evening Steps */
"templates.routine.sensitive.evening.step1.title" = "Krem Temizleyici";
"templates.routine.sensitive.evening.step1.why" = "Günün birikmiş kirlerini cildi tahriş etmeden veya koruyucu bariyerini tehlikeye atmadan nazikçe temizler";
"templates.routine.sensitive.evening.step1.how" = "Güneş kremi kullandıysanız çift temizleme yapın: önce yağ bazlı temizleyici, ardından krem temizleyici. Tahriş olmuş bölgelerin etrafında ekstra nazik olun.";

"templates.routine.sensitive.evening.step2.title" = "Yatıştırıcı Tonik";
"templates.routine.sensitive.evening.step2.why" = "Akşam uygulaması, gündüz oluşan tahrişi sakinleştirmeye yardımcı olur ve onarıcı bileşenlerin daha iyi emilmesi için cildi hazırlar";
"templates.routine.sensitive.evening.step2.how" = "Temiz cilde nazikçe tampon hareketlerle uygulayın. Gündüzden kaynaklanan kızarıklığı azaltmak için soğutucu, yatıştırıcı bileşenler kullanmanın tam zamanıdır.";

"templates.routine.sensitive.evening.step3.title" = "Hyaluronik Asit";
"templates.routine.sensitive.evening.step3.why" = "Gece hidrasyonu, tahriş veya hassasiyet riski olmadan siz uyurken bariyer onarımını destekler";
"templates.routine.sensitive.evening.step3.how" = "Hafif nemli cilde uygulayın. Maksimum nemlendirme ve gece boyunca iyileşme için bariyer kreminizin altına katmanlayın.";

"templates.routine.sensitive.evening.step4.title" = "Bariyer Onarıcı Krem";
"templates.routine.sensitive.evening.step4.why" = "Cilt, gece kendini onarır. Zengin bir bariyer kremi seramidler, kolesterol ve yağ asitleriyle bu süreci destekler";
"templates.routine.sensitive.evening.step4.how" = "Son adım olarak cömertçe uygulayın. Sabahkinden daha kalın bir katman kullanabilirsiniz. Daha sakin, daha güçlü bir ciltle uyanın.";

