/* Morning Steps */
"templates.routine.matureSkin.morning.step1.title" = "Nazik Temizleyici";
"templates.routine.matureSkin.morning.step1.why" = "Olgun cilt, yaşla birlikte azalan doğal yağları soymayan nazik temizlik gerektirir";
"templates.routine.matureSkin.morning.step1.how" = "Zengin krem veya yağ bazlı temizleyici kullanın. Ilık suyla 60 saniye nazikçe masaj yapın. Olgun cilt daha hassastır.";

"templates.routine.matureSkin.morning.step2.title" = "C Vitamini Serumu";
"templates.routine.matureSkin.morning.step2.why" = "Yaşlılık lekelerini aydınlatan, kolajeni destekleyen ve çevresel hasara karşı koruyan antioksidan gücü";
"templates.routine.matureSkin.morning.step2.how" = "Her sabah temiz cilde 4-5 damla uygulayın. Maksimum etkinlik için L-askorbik asit formülü kullanın. Serin ve karanlık yerde saklayın.";

"templates.routine.matureSkin.morning.step3.title" = "Hyaluronik Asit";
"templates.routine.matureSkin.morning.step3.why" = "Olgun cilt doğal olarak daha az hyaluronik asit üretir. Takviye etmek ince çizgileri dolgunlaştırır ve genç nemi geri kazandırır";
"templates.routine.matureSkin.morning.step3.how" = "Maksimum su bağlama için nemli cilde uygulayın. 3-4 damla kullanın. Nemi hapsetmek için hemen ardından nemlendirici sürün.";

"templates.routine.matureSkin.morning.step4.title" = "Zengin Göz Kremi";
"templates.routine.matureSkin.morning.step4.why" = "Göz çevresi yaşlanmayı ilk gösterir ve kaz ayakları, çukurluk ve kırışıklıklar için özel zengin bakıma ihtiyaç duyar";
"templates.routine.matureSkin.morning.step4.how" = "Yüzük parmağınızı kullanarak tüm göz çevresine cömert miktarda nazikçe dokunun. Cimrilik yapmayın - olgun gözler daha fazla ürüne ihtiyaç duyar.";

"templates.routine.matureSkin.morning.step5.title" = "Lüks Nemlendirici";
"templates.routine.matureSkin.morning.step5.why" = "Seramid ve peptit içeren zengin nemlendirici, yaşla birlikte zayıflayan bariyeri yeniden inşa eder, temel beslenmeyi sağlar";
"templates.routine.matureSkin.morning.step5.how" = "Yüz, boyun ve göğüs bölgesine cömertçe uygulayın. Bu bölgeler de yaşlanma belirtileri gösterir! Olgun cilt, genç cilde göre daha zengin formüllere ihtiyaç duyar.";

"templates.routine.matureSkin.morning.step6.title" = "Yaşlanma Karşıtı Güneş Kremi";
"templates.routine.matureSkin.morning.step6.why" = "SPF, EN önemli yaşlanma karşıtı adımdır. İlerideki hasarı önler ve cilt kanseri riskine karşı korur";
"templates.outine.matureSkin.morning.step6.how" = "SPF 50 geniş spektrumlu uygulayın. Yüz ve boyun için üç parmak uzunluğu kullanın. Dışarıda her 2 saatte bir yenileyin. Pazarlık kabul etmez!";

/* Evening Steps */
"templates.routine.matureSkin.evening.step1.title" = "Nazik Temizleyici";
"templates.routine.matureSkin.evening.step1.why" = "Akşam temizliği, günün çevresel hasarını temizler ve cildi güçlü gece bakımlarına hazırlar";
"templates.routine.matureSkin.evening.step1.how" = "Çift temizleme yapın: güneş kremi için önce yağ bazlı temizleyici, ardından krem temizleyici. Ekstra nazik olun - olgun cilt daha kolay morarır.";

"templates.routine.matureSkin.evening.step2.title" = "Retinol Bakımı";
"templates.routine.matureSkin.evening.step2.why" = "Yaşlanma karşıtı altın standart içerik. Hücre yenilenmesini artırır, kolajeni destekler, doku ve tonu iyileştirir";
"templates.routine.matureSkin.evening.step2.how" = "Retinole yeni başlıyorsanız %0,25 ile başlayın. Kuru cilde bezelye büyüklüğünde uygulayın. Nemlendiriciden önce 20 dakika bekleyin. Başlangıçta haftada 3-4 kez kullanın.";

"templates.routine.matureSkin.evening.step3.title" = "Peptit Kompleksi";
"templates.routine.matureSkin.evening.step3.why" = "Peptitler, cilde daha fazla kolajen ve elastin üretmesi için sinyal göndererek sıkılık ve yapı kaybını doğrudan hedefler";
"templates.routine.matureSkin.evening.step3.how" = "Retinol emildikten sonra 2-3 damla uygulayın. Peptitler, gelişmiş yaşlanma karşıtı faydalar için retinol ile sinerjik çalışır.";

"templates.routine.matureSkin.evening.step4.title" = "Hyaluronik Asit";
"templates.routine.matureSkin.evening.step4.why" = "Cilt onarım modundayken gece hidrasyonu. Çizgileri dolgunlaştırır ve olgun cildin kaybettiği esnekliği geri kazandırır";
"templates.routine.matureSkin.evening.step4.how" = "Hafif nemli cilde uygulayın. Sabahtan daha fazla miktarda kullanabilirsiniz. Olgun cildin nem ihtiyacı için esastır.";

"templates.routine.matureSkin.evening.step5.title" = "Zengin Göz Kremi";
"templates.routine.matureSkin.evening.step5.why" = "Retinol veya peptit içeren gece göz kremi, uyku sırasında kaz ayaklarını azaltmak ve hassas bölgeyi sıkılaştırmak için çalışır";
"templates.routine.matureSkin.evening.step5.how" = "Üst göz kapağı dahil tüm göz çevresine cömertçe uygulayın. Gece, bu içeriklerin sihrini gösterdiği zamandır.";

"templates.routine.matureSkin.evening.step6.title" = "Lüks Nemlendirici";
"templates.routine.matureSkin.evening.step6.why" = "Zengin gece kremi, seramidler, peptitler ve olgun cildin istediği besleyici yağlarla gece onarımını destekler";
"templates.routine.matureSkin.evening.step6.how" = "Yüz, boyun ve göğüs bölgesine kalın bir tabaka uygulayın. Ekstra beslenme için üzerine yüz yağı kullanabilirsiniz. Yenilenmiş bir ciltle uyanın.";
