"templates.routine.combination.morning.step4.title" = "Uyarlanabilir Nemlendirici";
"templates.routine.combination.morning.step4.why" = "Modern formüller, yüzünüzdeki farklı nem seviyelerine uyum sağlar, her bölgenin ihtiyacı olanı sunar";
"templates.routine.combination.morning.step4.how" = "T bölgesine ince bir tabaka, yanaklara daha cömert uygulayın. Veya iki ürün kullanın: yağlı bölgelere jel, kuru bölgelere krem.";

"templates.routine.combination.morning.step5.title" = "Geniş Spektrumlu Güneş Kremi";
"templates.routine.combination.morning.step5.why" = "Tüm bölgeler eşit UV korumasına ihtiyaç duyar. Hem yağlı hem de kuru bölgelere uygun bir formül seçin";
"templates.routine.combination.morning.step5.how" = "Hafif, yağsız SPF 30+ kullanın. Jel veya akışkan formüller en iyisidir. Tüm bölgelere eşit olarak uygulayın.";

/* Evening Steps */
"templates.routine.combination.evening.step1.title" = "Dengeleyici Temizleyici";
"templates.routine.combination.evening.step1.why" = "Günün değişken birikimini temizler - T bölgesinden fazla yağı, kuru bölgelerden çevresel stresi";
"templates.routine.combination.evening.step1.how" = "Gerekirse çift temizleme yapın. Yağ temizleyici güneş kremini, ardından dengeleyici temizleyiciyi temizler. Her bölgenin ihtiyaçlarına odaklanın.";

"templates.routine.combination.evening.step2.title" = "Tonik";
"templates.routine.combination.evening.step2.why" = "Akşam toniği daha hedefe yönelik olabilir - gerekirse farklı bölgeler için farklı tonikler kullanın";
"templates.routine.combination.evening.step2.how" = "Ellerinizle veya pamuk pedle uygulayın. Çoklu maskeleme için harika bir zamandır: yanaklara nemlendirici tonik, T bölgesine BHA.";

"templates.routine.combination.evening.step3.title" = "Hafif Serum";
"templates.routine.combination.evening.step3.why" = "Gece serumu, herhangi bir bölgeyi bunaltmadan tüm bölgelerdeki sorunları ele alır";
"templates.routine.combination.evening.step3.how" = "Tüm yüze uygulayın. Retinol veya niasinamid, karma cildin çeşitli ihtiyaçları için iyi çalışır.";

"templates.routine.combination.evening.step4.title" = "Bölgesel Bakım";
"templates.routine.combination.evening.step4.why" = "Burası özelleştirme yaptığınız yerdir - yağlı T bölgesi sorunlarını VE kuru yanak ihtiyaçlarını ayrı ayrı ele alın";
"templates.routine.combination.evening.step4.how" = "Yağlı/sivilce eğilimli bölgelere BHA veya leke bakımı uygulayın. Kuru bölgelere nemlendirici serum veya yağ kullanın. İhtiyaçlarınıza göre özelleştirin.";

"templates.routine.combination.evening.step5.title" = "Uyarlanabilir Nemlendirici";
"templates.routine.combination.evening.step5.why" = "Aşırı yağlanmaya veya kuruluğa neden olmayacak dengeli nemlendirme sağlarken bakımları hapseder";
"templates.routine.combination.evening.step5.how" = "Her bölgenin ihtiyacına göre uygulayın. T bölgesine ince bir tabaka, yanaklara daha zengin. Cildinizin geri bildirimini dinleyin.";

