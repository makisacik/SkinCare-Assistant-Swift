"templates.routine.minimalist.benefit4" = "Zaman açısından verimli";

/* Morning Steps */
"templates.routine.minimalist.morning.step1.title" = "Nazik Temizleyici";
"templates.routine.minimalist.morning.step1.why" = "Gece biriken yağı temizler ve cildi güne hazırlar. Sabahları çoğu kişinin ihtiyacı olan tek şey basit bir temizliktir";
"templates.routine.minimalist.morning.step1.how" = "Yüzü ıslatın, temizleyiciyi uygulayın, 30 saniye masaj yapın, ılık suyla durulayın, hafifçe kurulayın. Hepsi bu!";

"templates.routine.minimalist.morning.step2.title" = "Nemlendirici";
"templates.routine.minimalist.morning.step2.why" = "Cilt bariyerinizi nemlendirir ve korur. Yağlı cilde sahip olsanız bile tüm cilt tipleri için gereklidir";
"templates.routine.minimalist.morning.step2.how" = "Daha iyi emilim için cilt hafif nemliyken yüz ve boyun bölgesine bir miktar uygulayın. Emilmesini bekleyin.";

"templates.routine.minimalist.morning.step3.title" = "Güneş Kremi";
"templates.routine.minimalist.morning.step3.why" = "En önemli yaşlanma karşıtı adımdır. Kırışıklıklara, lekelere ve cilt kanserine neden olan UV hasarından korur";
"templates.routine.minimalist.morning.step3.how" = "Son sabah adımı olarak SPF 30+ uygulayın. Yüz ve boyun için yaklaşık iki parmak uzunluğu kullanın. Uzun süre dışarıdaysanız yenileyin.";

/* Evening Steps */
"templates.routine.minimalist.evening.step1.title" = "Nazik Temizleyici";
"templates.routine.minimalist.evening.step1.why" = "Cildinizin gece nefes alıp onarılabilmesi için günün birikmiş yağı, kiri, teri ve güneş kremini temizler";
"templates.routine.minimalist.evening.step1.how" = "Güneş kremi ve kirleri iyice temizlemek için 60 saniye masaj yapın. Ilık suyla iyice durulayın ve hafifçe kurulayın.";

"templates.routine.minimalist.evening.step2.title" = "Nemlendirici";
"templates.routine.minimalist.evening.step2.why" = "Gece nemlendirmesi, siz uyurken cildinizin doğal onarım sürecini destekler. Sabah versiyonundan biraz daha zengin olabilir";
"templates.routine.minimalist.evening.step2.how" = "Temiz, hafif nemli cilde uygulayın. Nazik, yukarı doğru hareketler kullanın. Cildinizin besinleri emmesi için mükemmel bir zamandır.";

