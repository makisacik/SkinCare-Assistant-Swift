/* Individual Quotes */
"discover.quotes.quote1.text" = "Güzellik yüzde değildir; güzellik, kalpte yanan bir ışıktır.";
"discover.quotes.quote1.author" = "Kahlil Gibran";
"discover.quotes.quote1.category" = "Felsefe";

"discover.quotes.quote2.text" = "Cildin en büyük organındır. Ona sevgi ve sabırla bak.";
"discover.quotes.quote2.author" = "Dr. Howard Murad";
"discover.quotes.quote2.category" = "Cilt Bakımı";

"discover.quotes.quote3.text" = "Gerçek güzellik mükemmellik hakkında değil, kendine özgü benliğini kucaklamakla ilgilidir.";
"discover.quotes.quote3.author" = "Alicia Keys";
"discover.quotes.quote3.category" = "Öz Sevgi";

"discover.quotes.quote4.text" = "Cildine bakımın sessizliğiyle nefes aldır, parlaklığın kendine güven fısıldasın.";
"discover.quotes.quote4.author" = "Bilinmiyor";
"discover.quotes.quote4.category" = "Cilt Bakımı";

"discover.quotes.quote5.text" = "Güzellik, nezaket anlarında yaşar — yumuşak bir dokunuş, sakin bir nefes, sabırlı bir kalp.";
"discover.quotes.quote5.author" = "Bilinmiyor";
"discover.quotes.quote5.category" = "Güzellik";

"discover.quotes.quote6.text" = "Cildine bir hatıraya davrandığın gibi davran — şefkatle, acele etmeden, sevgiyle.";
"discover.quotes.quote6.author" = "Bilinmiyor";
"discover.quotes.quote6.category" = "Felsefe";

"discover.quotes.quote7.text" = "Vücudunu dinlemeyi öğrendiğinde sessizce çiçek açarsın.";
"discover.quotes.quote7.author" = "Bilinmiyor";
"discover.quotes.quote7.category" = "Öz Sevgi";

"discover.quotes.quote8.text" = "Işıltı, acele etmekten değil, dinlenmekten doğar.";
"discover.quotes.quote8.author" = "Bilinmiyor";
"discover.quotes.quote8.category" = "Güzellik";

"discover.quotes.quote9.text" = "Cildine bakarken, zamanı yavaşlatma sanatını uygularsın.";
"discover.quotes.quote9.author" = "Bilinmiyor";
"discover.quotes.quote9.category" = "Cilt Bakımı";

"discover.quotes.quote10.text" = "Aradığın ışıltı, koruduğun huzurdur.";
"discover.quotes.quote10.author" = "Bilinmiyor";
"discover.quotes.quote10.category" = "Öz Sevgi";

"discover.quotes.quote11.text" = "Güzellik görülmeyi istemez — sadece yumuşakça var olur.";
"discover.quotes.quote11.author" = "Bilinmiyor";
"discover.quotes.quote11.category" = "Felsefe";

"discover.quotes.quote12.text" = "Her bakım ritüeli, kendinle sessiz bir konuşmadır.";
"discover.quotes.quote12.author" = "Bilinmiyor";
"discover.quotes.quote12.category" = "Cilt Bakımı";

"discover.quotes.quote13.text" = "Yansıman, gördüğün değil, ne kadar nazik baktığındır.";
"discover.quotes.quote13.author" = "Bilinmiyor";
"discover.quotes.quote13.category" = "Öz Sevgi";

"discover.quotes.quote14.text" = "Mutluluk en iyi makyajdır. Önce iç parlaklığına bak.";
"discover.quotes.quote14.author" = "Drew Barrymore";
"discover.quotes.quote14.category" = "Öz Sevgi";

"discover.quotes.quote15.text" = "Kendine güven, güzellik doğurur. İyi hissettiğinde, daha da güzel görünürsün.";
"discover.quotes.quote15.author" = "Blake Lively";
"discover.quotes.quote15.category" = "Güzellik";

"discover.quotes.quote16.text" = "Cildine bakmak, onu örtmekten daha önemlidir.";
"discover.quotes.quote16.author" = "Bilinmiyor";
"discover.quotes.quote16.category" = "Cilt Bakımı";

"discover.quotes.quote17.text" = "Öz bakım, gücünü geri almanın yoludur.";
"discover.quotes.quote17.author" = "Lalah Delia";
"discover.quotes.quote17.category" = "Öz Sevgi";
